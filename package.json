{
  "name": "@teamemory/components",
  "version": "0.0.1",
  "description": "A Vue3 component library based on Element Plus",
  "main": "./dist/teamemory-components.cjs.js",
  "module": "./dist/teamemory-components.es.js",
  "browser": "./dist/teamemory-components.umd.js",
  "exports": {
    ".": {
      "import": "./dist/teamemory-components.es.js",
      "require": "./dist/teamemory-components.cjs.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite serve examples",
    "build": "rimraf dist && rollup -c",
    "build:docs": "vite build docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "new": "node scripts/cli.js",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "prepublishOnly": "npm run build",
    "preview": "vite preview examples"
  },
  "keywords": [
    "vue",
    "component-library",
    "ui",
    "element-plus",
    "emotion"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.4",
    "element-plus": "^2.3.6",
    "@emotion/css": "^11.11.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1"
  }
}