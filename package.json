{
  "name": "@teamemory/components",
  "version": "0.0.1",
  "description": "A Vue3 component library based on Element Plus",
  "main": "dist/teamemory-components.cjs.js",
  "module": "dist/teamemory-components.es.js",
  "browser": "dist/teamemory-components.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/teamemory-components.es.js",
      "require": "./dist/teamemory-components.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "dev": "vite serve examples",
    "build": "rimraf dist && run-s build:lib build:types",
    "build:lib": "rollup -c rollup.config.mjs",
    "build:types": "dts-bundle-generator -o dist/types/index.d.ts src/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "new": "node scripts/cli.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx,.vue",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui",
    "element-plus",
    "emotion",
    "teamemory"
  ],
  "author": "Teamemory",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.4",
    "element-plus": "^2.3.6"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chokidar": "^3.5.3",
    "dts-bundle-generator": "^8.0.1",
    "element-plus": "^2.3.6",
    "eslint": "^8.42.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "vite": "^4.3.9",
    "vitepress": "^1.0.0-alpha.76",
    "vitest": "^0.31.1",
    "vue": "^3.3.4",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teamemory/components.git"
  },
  "bugs": {
    "url": "https://github.com/teamemory/components/issues"
  },
  "homepage": "https://github.com/teamemory/components#readme"
}